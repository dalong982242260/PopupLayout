<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PopupLayout">
        <attr name="animDuration" format="integer" />
        <attr name="shadowBackgroundColor" format="color" />
        <attr name="isAnim" format="boolean" />
        <attr name="canceledOnTouchOutside" format="boolean" />
        <attr name="animType">
            <enum name="anim_top" value="0" />
            <enum name="anim_right" value="1" />
            <enum name="anim_bottom" value="2" />
            <enum name="anim_left" value="3" />
        </attr>
    </declare-styleable>
</resources>